
<div class="pull-right">
  <b>(<a href="/extented_stats_hide">leave raw stats view</a>)</b>
</div>
</br>

<b>Server</b>
<table class="table table-bordered table-striped">
  <tbody>
    <% @cur_sdk_stat.each do |k,v| %>
      <tr>
        <td><%= k %></td>
        <td><%= v %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<% @agents.each do |agent_name, agent| %>
  <% if agent.running %>
    <b>Agent <%= agent_name %></b>

    <table class="table table-bordered table-striped">
      <tbody>
        <% agent.agent_stats.each do |k,v| %>
          <tr>
            <td><%= k %></td>
            <td><%= v %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  <% end %>
<% end %>



<% @cur_sdk_stats_full['response_time']['last_hour_stats'].each do |k, v| %>
  <b>Response time - last hour - '<%= k %>'</b>
  <table class="table table-bordered table-striped">
  <tbody>
    <tr>
      <td><%= 'ref hour' %></td>
      <td><%= v['ref_hour'] %></td>
    </tr>
    <tr>
      <td><%= 'spectrum' %></td>
      <td><%= v['spectrum'] %></td>
    </tr>
    <tr>
      <td><%= 'value' %></td>
      <td><%= v['value'] %></td>
    </tr>
  </tbody>
  </table>
<% end %>

<% @cur_sdk_stats_full['response_time']['last_day_stats'].each do |k, v| %>
  <b>Response time - last day - '<%= k %>'</b>
  <table class="table table-bordered table-striped">
  <tbody>
    <tr>
      <td><%= 'spectrum' %></td>
      <td><%= v %></td>
    </tr>
    <tr>
      <td><%= 'value' %></td>
      <td><%= v %></td>
    </tr>
  </tbody>
  </table>
<% end %>
