<head>
  <title>Ruby agents sdk admin</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <!-- Bootstrap -->
  <link href="/css/bootstrap.min.css" rel="stylesheet" media="screen">
  <link href="/css/server_admin.css" rel="stylesheet" media="screen">

  <!-- Highlight in documentation -->
  <link href="/css/railcast.css" rel="stylesheet" media="screen">
  <link href="/css/bootstrap-responsive.min.css" rel="stylesheet">

  <script src="http://code.jquery.com/jquery.js"></script>
  <script src="/js/highlight.js"></script>
  <script src="/js/bootstrap.min.js"></script>
  <meta charset="utf-8" />

  <style>
  body {
    padding-top: 60px; /* 60px to make the container go all the way to the bottom of the topbar */
  }
  </style>
</head>



<body data-spy="scroll" data-target=".bs-docs-sidebar">
  <div class="navbar navbar-inverse navbar-fixed-top">
    <div class="navbar-inner">
      <div class="container">
        <div class="nav-collapse collapse">
          <ul class="nav">
            <li class="">
              <a href="#">SDK Server <%= get_sdk_version %></a>
            </li>
            <li class="">
            <li class="<% if @active_tab=='projects'%><%= 'active' %><% end %>">
              <a href="./projects"><i class="icon-th-large icon-white"></i> SDK Agents</a>
            </li>
            <li class="<% if @active_tab=='doc'%><%= 'active' %><% end %>">
              <a href="./doc"><i class="icon-book icon-white"></i> Documentation</a>
            </li>
            <li class="<% if @active_tab=='patch_note'%><%= 'active' %><% end %>">
              <a href="./patch_note"><i class="icon-book icon-white"></i> Patch notes</a>
            </li>
            <li class="<% if @active_tab=='logSdk'%><%= 'active' %><% end %>">
              <a href="./logSdk#endlog"><i class="icon-wrench icon-white"></i> SDK Server Log</a>
            </li>
            <li class="<% if @active_tab=='logSdkAgents'%><%= 'active' %><% end %>">
              <a href="./logSdkAgents#endlog"><i class="icon-wrench icon-white"></i> SDK Agents Log</a>
            </li>
          </ul>
        </div>
      </div>
    </div>
  </div>

  <div class="container">
    <%= yield %>
  </div>

</body>
